<CustButton@Button>:
    size_hint:None,None
    background_normal:''
    background_color:(0.3,0.65,1,0)

    BoxLayout:
        id:layout
        size_hint_y:None
        size: self.parent.size
        orientation:'vertical'
        padding:4
        pos: self.parent.pos
<PropCheckBox@CheckBox>
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos:self.center_x-8, self.center_y-8
            size:16,16
    size_hint_y:None
    height:25

<Select_Button>:
    size_hint_y:None
    size:100,25
    font_size:15
    background_normal:''
    background_color: 0.25,0.25,0.25,1
    canvas:
        Color:
            rgba:0,0,0,1
        Line:
            rectangle:self.pos[0],self.pos[1],self.size[0],self.size[1]
<PropInputLabel@Label>:
    size_hint_y:None
    height:25
    text_size: self.size
    halign: 'left'
    color:0,0,0,1
<PropInputTextInput@TextInput>:
    size_hint_y:None
    height:25
    font_size:12
<PLabel@Label>:
    size_hint_y:None
    height:25
    text_size: self.size
    halign: 'left'
    color:0,0,0,1
<PTextInput@TextInput>:
    size_hint_y:None
    height:25
<PButton@Button>
    size_hint_y:None
    size:100,25
    font_size:15
    background_normal:''
    background_color: 0.25,0.25,0.25,1
    canvas:
        Color:
            rgba:0,0,0,1
        Line:
            rectangle:self.pos[0],self.pos[1],self.size[0],self.size[1]

<PCheckBox@CheckBox>
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos:self.center_x-8, self.center_y-8
            size:16,16
    size_hint_x:None
    width:25


